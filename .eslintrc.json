{
  "extends": "airbnb-base",
  "plugins": [
    "import"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "class-methods-use-this": [
      "off"
    ],
    "comma-dangle": [
      "off"
    ],
    "import/no-extraneous-dependencies": [
      "off"
    ],
    "max-len": [
      "off"
    ],
    "no-restricted-syntax": [
      "off"
    ],
    "no-underscore-dangle": [
      "off"
    ],
    "no-use-before-define": [
      "off"
    ]
  },
  "globals": {
    "$": true,
    "recompose": true
  }
}
